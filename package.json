{
  "name": "prisma-serverless-prisma-mongo",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -e .env -- node build/index.js",
    "dev": "dotenv -e .env.development -- nodemon --watch src/ --exec ts-node src/index.ts",
    "prisma-generate-dev": "dotenv -e .env.development -- npx prisma db push && dotenv -e .env.development -- npx prisma generate",
    "prisma-generate": "npx prisma db push && npx prisma generate",
    "deploy": "dotenv -e .env -- serverless deploy",
    "deploy-dev": "dotenv -e .env.development -- serverless deploy --stage dev"
  },
  "dependencies": {
    "@prisma/client": "4.9.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "1.4.6",
    "jsonwebtoken": "9.0.0",
    "just-is-empty": "3.3.0",
    "just-omit": "2.2.0",
    "node-ts-cache": "4.4.0",
    "node-ts-cache-storage-memory": "4.4.0",
    "tsoa": "5.0.0",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.110",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.16",
    "@types/jsonwebtoken": "9.0.1",
    "@types/morgan": "1.9.4",
    "@types/node": "18.11.18",
    "@types/swagger-ui-express": "4.1.3",
    "aws-lambda": "1.0.7",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.0.0",
    "esbuild": "0.16.17",
    "express": "4.18.2",
    "morgan": "1.10.0",
    "nodemon": "2.0.20",
    "prisma": "4.9.0",
    "serverless": "3.27.0",
    "serverless-dotenv-plugin": "4.0.2",
    "serverless-esbuild": "1.37.3",
    "serverless-offline": "12.0.4",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-ssm-fetch": "^2.0.0",
    "swagger-ui-express": "4.6.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "winston": "3.8.2"
  }
}
